<!DOCTYPE html>
<html>
    <head>   
      <%- include('./layout/head'); %> 
    </head>

    <body>
    
    <%- include('./layout/header'); %> 
    
	<main role="main">

        <div class="jumbotron">
			<div class="container">
			  <h1 class="display-3">User</h1>
			  
			</div>
		</div>


        <div class="container" style="text-align: center;" >
                <div class="row">
                    <%users.forEach(function(user) { %>
                        <div class="row">
                        <p><%- user.username %></p>
                        <p><%- user.password %></p>
                        </div>
                </div>
        </div>
    </main>
          

    </body>
</html> 